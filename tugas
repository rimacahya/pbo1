    void penjumlahan(){
        
       nilaiA=Integer.parseInt(jTextField1.getText());
       nilaiB=Integer.parseInt(jTextField2.getText());
        
       nilaiHasil=nilaiA+nilaiB;
  
         jLabel4.setText(" "+nilaiHasil);
        
    }
        void pembagian (){
            nilaiA1=Double.parseDouble(jTextField1.getText());
            nilaiB1=Double.parseDouble(jTextField2.getText());
            
            nilaiHasil1=nilaiA1/nilaiB1;
            
            jLabel4.setText(" "+nilaiHasil1);
        }
        
        
        void pengurangan(){
        
        nilai1=Integer.parseInt(jTextField1.getText());
        nilai2=Integer.parseInt(jTextField2.getText());
        hasil=nilai1-nilai2;
        
        jLabel4.setText(" "+hasil);
        
        }
        void perkalian(){
        
        nilai12=Integer.parseInt(jTextField1.getText());
        nilai13=Integer.parseInt(jTextField2.getText());
        hasil1=nilai12*nilai13;
        
        jLabel4.setText(" "+hasil1);
        
        }

